  "suggest": {
    "illuminate/validation": "^9.0"
  },
  "extra": {
    "laravel": {
      "aliases": {
      },
      "providers": [
        "SzuniSoft\\SzamlazzHu\\Providers\\SzamlazzHuServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "phpunit"
  }
