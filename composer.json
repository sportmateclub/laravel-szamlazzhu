{
  "name": "sportmateclub/laravel-szamlazzhu",
  "license": "MIT",
  "description": "Szamlazz.hu integration for Laravel",
  "keywords": [
    "laravel",
    "invoice",
    "receipt",
    "szamlazz.hu",
    "szamlazzhu",
    "currency"
  ],
  "authors": [
    {
      "name": "Forg√°cs Bence",
      "email": "sykorax93@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-simplexml": "*",
    "ext-xmlwriter": "*",
    "guzzlehttp/guzzle": "7.*",
    "laravel/framework": "9.*"
  },
  "require-dev": {
    "orchestra/testbench": "*",
    "phpunit/phpunit": "*",
    "mockery/mockery": "^1.4"
  },
  "autoload": {
    "files": [
